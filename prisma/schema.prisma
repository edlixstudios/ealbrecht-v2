generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model myskills {
  id     String  @id @default(uuid())
  title  String
  skills skill[]
}

model skill {
  id         Int       @id @default(autoincrement())
  name       String
  value      Int
  myskills   myskills? @relation(fields: [myskillsId], references: [id])
  myskillsId String?
}
